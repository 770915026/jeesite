html {
	width: 100%;
	overflow-x:hidden;
}
body{
	background:#fff;
}
.container{
    width: 100%;
}
.taier-container{
	width:1149px;
	margin:0 auto;
}
.taier-container-2{
	width:1200px;
	margin:0 auto;
}
.taier-container-coa{
	width:1187px;
	margin:0 auto;
}

/*主体*/
.public-list{

}
.public-list .list-box{
	
}
.public-list .list-box .header-banner{
	width:1920px;
	height: 608px;
	overflow:hidden;
	position: relative;
}
/*头部焦点图*/
.public-list .list-box .banner-flash{
	width:100%;
	height: 608px;
	position: relative;
}
.public-list .list-box .banner-flash li{
	display: inline-block;
	float:left;
	width:100%;
	height: 100%;
}
.public-list .list-box .banner-flash li img{
	width:100%;
	height: 100%;
}
.public-list .list-box .banner-text{
	width: 100%;
	text-align: center;
	position: absolute;
	left:0;
	top:240px;
}
.public-list .list-box .banner-text strong{
	display: block;
	font-size: 91px;
	color:#fff;
	font-weight: normal;
}
.public-list .list-box .banner-text em{
	display: block;
	font-size: 30px;
	color: #fff;
	margin-top: 10px;
}
.public-list .list-box .focus-box{
	text-align: center;
	position: absolute;
	left:50%;
	top:520px;
	z-index: 100;
	margin-left: -30px;
}
.public-list .list-box .focus-box li{
	display: inline-block;
	width:12px;
	height: 12px;
	border-radius: 100px;
	background:#6b7474;
	text-indent: -99999px;
	margin-right: 5px;
	cursor: pointer;
}
.public-list .list-box .focus-box li.current{
	background:#4285ff;
}
.public-list .list-box .pointer{
	display: inline-block;
	position: absolute;
	left:50%;
	bottom: 10px;
	z-index: 100;
	width: 70px;
	height: 50px;
	margin-left: -35px;
}
/*泰尔头条*/
.public-list .list-box .taier-head-lines{
	height: 50px;
	line-height: 50px;
	border-bottom: 1px solid #e8e8e8;
	box-sizing: border-box;
}
.public-list .list-box .taier-head-lines .taier-news{
	width: 1149px;
	margin:0 auto;
}

.public-list .list-box .taier-head-lines .taier-news .taier-news-public{
	display: inline-block;
	position: relative;
	width:950px;
	height: 50px;
	overflow:hidden;
	float: left;
}
.public-list .list-box .taier-head-lines .taier-news .taier-news-public .public_ul {
	position: absolute;
	left: 0;
	top: 0;
	height: 50px;
}
.public-list .list-box .taier-head-lines .taier-news .taier-news-public .public_ul li{
	display: inline-block;
	float: left;
	padding:0;
	height: 50px;
}

.public-list .list-box .taier-head-lines .taier-news .taier-news-public .public_ul li .news{
	display: inline-block;
	/*background:url(../images/taier-news.jpg) left 9px no-repeat;
	padding-left: 105px;*/
	font-size: 14px;
	color:#2b2f32;
	text-decoration: none;
	cursor: pointer;
	position: relative;
}

.public-list .list-box .taier-head-lines .taier-news .taier-news-tit{
	display: inline-block;
	float: left;
	width:97px;
	height: 26px;
	text-indent: -9999px;
	margin-top: 9px;
	margin-right: 10px;
	background:url(../images/taier-news.jpg) left center no-repeat;
}

.public-list .list-box .taier-head-lines .taier-news .news .line{
	color:#c5c5c5;
	margin:0 12px;
}
.public-list .list-box .taier-head-lines .taier-news .news .icon{
	position: absolute;
	right: -30px;
	top: 7px;
	width: 15px;
	height: 15px;
	background:url(../images/taier-ico01.jpg) right center no-repeat;
}
.public-list .list-box .taier-head-lines .more{
	float:right;
	font-size: 14px;
	color:#2b2f32;
	text-decoration: none;
	cursor: pointer;
}
/*泰尔权威证书*/
.taier-coa-wrap{
	height: 709px;
	background:url(../images/index-bg03.jpg) top center no-repeat;
}
.taier-coa .coa-tit{
	display: block;
	padding-top: 80px;
	font-size: 30px;
	color:#fff;
	font-weight: normal;
	text-align: center;
}
.taier-coa .taier-coa-nav{
	text-align: center;
}
.taier-coa .taier-coa-nav span{
	display: inline-block;
	width:182px;
	height: 60px;
	line-height: 60px;
	text-align: center;
	font-size: 20px;
	color:#fff;
	background:#2b2f32;
	margin-top: 44px;
	cursor: pointer;
}
.taier-coa .taier-coa-nav span.current{
	background:#4285ff;
}
.taier-coa .taier-coa-tab{
	margin-top: 49px;
}
.taier-coa .taier-coa-tab{
	display: none;
}
.taier-coa .taier-coa-tab.current{
	display: block;
}
.taier-coa .taier-coa-tab dd{
	display: inline-block;
	float:left;
	width:236px;
	height: 337px;
	overflow:hidden;
}
.taier-coa .taier-coa-tab dd img{
	width:236px;
	height: 337px;
}
/*优势与流程*/
.taier-coa .advantage-tab{
	height: 378px;
	background:url(../images/index-bg04.png) right center no-repeat;
}
.taier-coa .advantage-tab .advantage-text{
	width: 725px;
	padding-top: 40px;
}
.taier-coa .advantage-tab .advantage-text p{
	font-size: 18px;
	color:#fff;
	line-height: 25px;
	margin-top: 25px;
	background:url(../images/taier-ico11.jpg) left 5px no-repeat;
	padding-left:10px;
}									
.taier-coa .advantage-tab .advantage-text .more{
	display: inline-block;
	float: right;
	width:165px;
	height: 46px;
	line-height: 46px;
	font-size:16px;
	color:#fff;
	text-align: center;
	border:1px solid #fff;
	margin-top: 26px;
}


/*企业信息发布*/
.enterprise-news-wrap{
	height: 765px;
	background:#fff;
}
.enterprise-news-wrap .enterprise-news{
	text-align: center;
}
.enterprise-news-wrap .enterprise-news .news-title{
	display: block;
	font-size: 30px;
	color:#2b2f32;
	margin-top: 80px;
}
.enterprise-news-wrap .enterprise-news .news-text{
	display: block;
	font-size: 20px;
	color:#2b2f32;
	margin-top: 15px;
}
.enterprise-news-wrap .enterprise-news .news-bg-img{
	width: 626px;
	height: 269px;
	margin: 70px auto 0;
	background:url(../images/taier-ico08.jpg) center center no-repeat;
}
.enterprise-news-wrap .enterprise-news .enterprise-news-box{
	height: 170px;
	position: relative;
}
.enterprise-news-wrap .enterprise-news .enterprise-news-box .arrow-left{
	display: inline-block;
	position: absolute;
	left:0;
	top:50%;
	width:29px;
	height: 29px;
	background:url(../images/taier-left-arrow.jpg) no-repeat;
	text-indent: -99999px;
	cursor: pointer;
}
.enterprise-news-wrap .enterprise-news .enterprise-news-box .arrow-right{
	display: inline-block;
	position: absolute;
	right:0;
	top:50%;
	width:29px;
	height: 29px;
	background:url(../images/taier-right-arrow.jpg) no-repeat;
	text-indent: -99999px;
	cursor: pointer;
}
.enterprise-news-wrap .enterprise-news .enterprise-news-box .enterprise-news-list{
	width:985px;
	margin:25px auto 0;
	position: relative;
	overflow:hidden;
	height: 160px;
}
.enterprise-news-wrap .enterprise-news .enterprise-news-box .enterprise-news-list dl{
	position: relative;
    width:900000px;
}
.enterprise-news-wrap .enterprise-news .enterprise-news-box .enterprise-news-list dd{
	display: inline-block;
	float: left;
	width: 172px;
	height: 160px;
	margin: 0 0 0 25px;
}
.enterprise-news-wrap .enterprise-news .enterprise-news-box .enterprise-news-list dd span{
	display: inline-block;
	float: left;
	width: 172px;
	height: 55px;
	margin: 25px 0 0 0;
}
/*行业分析报告*/
.taier-report-wrap{
	height: 547px;
	background:url(../images/index-bg02.jpg) top center no-repeat;
}
.taier-report{
	text-align: center;
}
.taier-report .report-tit{
	display: block;
	padding-top: 106px;
	font-size: 30px;
	color:#fff;
	font-weight: normal;
}
.taier-report .report-text{
	display: block;
	padding-top: 20px;
	font-size: 20px;
	color:#fff;
}
.report-ul{
	text-align: center;
    display: inline-block;
    margin-top: 83px;
}
.report-ul li{
	display: inline-block;
	float:left;
	width: 167px;
	height: 98px;
	margin-right: 5px;
	text-indent: -9999px;
}
.report-ul li:nth-last-of-type(1){
	margin-right:0;
}
.report-ul li a{
	display: inline-block;
	width: 167px;
	font-size: 18px;
	color:#fff;
}
.report-ul li a img{
	display: inline-block;
	width: 165px;
	height: 97px;
	border:1px solid #526eff;
	margin-bottom: 10px;
}
/*泰尔视角聚焦业内热点话题*/
.taier-hotspot-wrap{
	height: 619px;
	background:url(../images/taier-hotspot-bg.jpg) top center no-repeat;
}
.taier-hotspot-wrap .taier-hotspot{

}
.taier-hotspot-wrap .taier-hotspot .title{
	font-size:34px;
	text-align: center;
	padding-top: 100px;
	font-weight: normal;
}
.taier-hotspot-wrap .taier-hotspot .taier-news-nav{
	margin-top: 40px;
	text-align: center;
}
.taier-hotspot-wrap .taier-hotspot .taier-news-nav span{
	display: inline-block;
	font-size: 18px;
	color:#000;
	margin:0 15px 0 15px;
	cursor:pointer;
}
.taier-hotspot-wrap .taier-hotspot .taier-news-nav span.current{
	color:#0071ff;
}
.taier-hotspot-wrap .taier-hotspot .taier-news-tab{
	display: none;
}
.taier-hotspot-wrap .taier-hotspot .taier-news-tab.current{
	display: block;
}
.taier-hotspot-wrap .taier-hotspot .taier-news-tab li{
	display: inline-block;
	float:left;
	width: 340px;
	height: 210px;
	margin:48px 0 0 53px;
}
.taier-hotspot-wrap .taier-hotspot .taier-news-tab li.list{
	margin-left: 15px;
}
.taier-hotspot-wrap .taier-hotspot .taier-news-tab li a{
	display: inline-block;
	text-decoration: none;
}
.taier-hotspot-wrap .taier-hotspot .taier-news-tab li .img-box{
	width: 340px;
	height: 158px;
	position: relative;
}
.taier-hotspot-wrap .taier-hotspot .taier-news-tab li .img-box img{

}
.taier-hotspot-wrap .taier-hotspot .taier-news-tab li .img-box .opac{
	width: 100%;
	position: absolute;
	bottom:2px;
	left:0;
	height: 37px;
	background:#000;
	filter:alpha(opacity=50);-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;
}
.taier-hotspot-wrap .taier-hotspot .taier-news-tab li .img-box .text{
	width: 100%;
	position: absolute;
	bottom:2px;
	left:0;
	height: 37px;
	line-height: 37px;
	font-size: 16px;
	text-align: center;
	color:#fff;
}
.taier-hotspot-wrap .taier-hotspot .taier-news-tab li .news-text{
	font-size:14px;
	color:#000;
	height: 52px;
	line-height: 24px;
	margin-top: 10px;
	text-overflow: -o-ellipsis-lastline;  
	overflow: hidden;  
	text-overflow: ellipsis;  
	display: -webkit-box;  
	-webkit-line-clamp: 2;  
	-webkit-box-orient: vertical;  
}
.taier-hotspot-wrap .taier-hotspot .more-con{
	display: block;
	margin:55px auto 0;
	width:94px;
}
.taier-hotspot-wrap .taier-hotspot .more-con .more{
	display: inline-block;
	width:94px;
	height: 30px;
	background:url(../images/taier-ico07.png) left center no-repeat;
}










@media(max-width: 1920px){
	.header-box .public-nav{
		width: 60%;
	}
}
@media(max-width: 1680px){
	.header-box .public-nav{
		width: 60%;
	}
}
@media(max-width: 1440px){
	.header-box .public-nav{
		width: 68%;
	}
	.header-box .header-box .taier-logo{
		margin-top: 5px;
	}
	.header-box .public-nav dd.current a:after{
	content: "";
	left:-10px;
    padding: 0 10px;
	}
}
@media(max-width: 1366px){
	.header-box .public-nav{
		width: 68%;
	}
	.header-box .public-nav dd{
		margin-right: 15px;
	}
	.header-box .header-box .taier-logo{
		margin-top: 10px;
	}
	.header-box .public-nav dd.current a:after{
	content: "";
	left:-10px;
    padding: 0 10px;
	}
}
@media(max-width: 1280px){
	.header-box .public-nav{
		width: 67%;
	}
	.header-box .public-nav dd{
		margin-right: 15px;
	}
	.header-box .public-nav dd a{
		font-size: 14px;
	}
	.header-box .public-nav dd.current a:after{
	content: "";
	left:-10px;
    padding: 0 10px;
	}
}
@media (max-width: 1110px){
	.header-box .public-nav{
		width: 58%;
	}
	.header-box .public-nav dd{
		margin-right: 10px;
	}
	.header-box .public-nav dd a{
		font-size: 12px;
	}
	.header-box .public-nav dd.current a:after{
	content: "";
	left:-5px;
    padding: 0 5px;
	}
}
@media(max-width: 1024px){
	.header-box .public-nav{
		width: 58%;
	}
	.header-box .public-nav dd{
		margin-right: 5px;
	}
	.header-box .public-nav dd a{
		font-size: 12px;
	}
	.header-box .public-nav dd.current a:after{
	content: "";
	left:-5px;
    padding: 0 5px;
	}
}













